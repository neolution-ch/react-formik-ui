{
  "name": "@neolution-ch/react-formik-ui",
  "version": "1.0.0",
  "description": "Common Form Elements",
  "license": "MIT",
  "author": "Neolution",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist && microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react --globals react/jsx-runtime=jsx --no-compress --tsconfig ./tsconfig.microbundle.json",
    "build-storybook": "build-storybook",
    "lint": "eslint \"**/*.{ts,tsx}\" --cache --max-warnings 0",
    "prepack": "yarn build",
    "prepare-pr": "yarn prettier . --write && yarn lint && yarn build && yarn test\"",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "start": "microbundle watch --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react --globals react/jsx-runtime=jsx --no-compress --tsconfig ./tsconfig.microbundle.json",
    "start-all": "concurrently \"yarn start\" \"yarn start-yalc\"",
    "start-yalc": "yarn nodemon --watch dist -x \"yarn yalc push\"",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env DEBUG_PRINT_LIMIT=100 jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "react-number-format": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/reactstrap": "^8.7.2",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-loader": "^8.2.4",
    "bootstrap": "^4.0.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.5.8",
    "formik": "^2.2.6",
    "gh-pages": "^3.2.3",
    "jest": "^27.5.1",
    "microbundle": "^0.15.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^8.9.0",
    "shx": "^0.3.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.2.3",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "formik": "^2.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reactstrap": "^8.9.0"
  },
  "engines": {
    "node": ">=14"
  }
}
